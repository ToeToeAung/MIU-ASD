package CameraApp;

public class StolenDetectionHandler {

}
public class StolenDetectionHandler extends DetectionHandler {

	public StolenDetectionHandler(CameraRecord record) {
		super(record);
	}

	@Override
	public void detect() {
		if (record.licensePlate.contains("ABC 1234")) {
			System.out.println("Stolen detected!");
		} else {
			DetectionHandler nextHandle = new SpeedingDetect(record);
			nextHandle.detect();
		}
	}
}
