package webshop;

public class Product  implements ProductCatalogComponent{
	  private String name;

	    public Product(String name) {
	        this.name = name;
	    }
	    
	    public double getPrice() {
			return price;
		}



	    @Override
	    public void print() {
	    	System.out.println("Product: " + this.name + ", price: " + getPrice());
	    }
}
